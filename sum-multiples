class Solution:
    def sumOfMultiples(self, n: int) -> int:
        # Initialize the sum
        total_sum = 0
        
        # Iterate through the range [1, n]
        for num in range(1, n+1):
            # Check if the number is divisible by 3, 5, or 7
            if num % 3 == 0 or num % 5 == 0 or num % 7 == 0:
                # Add the number to the sum
                total_sum += num
        
        return total_sum
